                        function settime(id) {
                            $('[id^="reqDataXY"]').prop("disabled", true);
                            $('[id^="reqDataXY-' + id + '"]').prop("disabled", false);
                            $("#reqMulai-" + id).hide();
                            $("#reqStop-" + id).show();

                            clearInterval(interval);
                            clearTimeout(interval);
                            localStorage.setItem("end", null);
                            localStorage.clear();

                            // localStorage.removeItem("end"+id);
